#reader(lib "htdp-beginner-reader.ss" "lang")((modname ex2-2-2) (read-case-sensitive #t) (teachpacks ((lib "convert.rkt" "teachpack" "htdp"))) (htdp-settings #(#t constructor repeating-decimal #f #t none #f ((lib "convert.rkt" "teachpack" "htdp")) #f)))
(define (Fahrenheit->Celsius fahrenheitTemp)
  (* (- fahrenheitTemp 32) (/ 5 9)))

(define (FToC fTemp)
  (Fahrenheit->Celsius fTemp))